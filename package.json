    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "node-cron": "^2.0.3",
    "node-sass": "^4.14.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
